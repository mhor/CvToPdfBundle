{
    "name": "mhor/cv-to-pdf-bundle",
    "type": "symfony-bundle",
    "description": "Symfony2 bundle to convert a CV to a beautiful PDF",
    "homepage": "https://github.com/mhor/CvToPdfBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "mhor",
            "homepage": "https://github.com/mhor/"
        }
    ],
    "autoload": {
        "psr-0": { "Mhor\\CvToPdfBundle" : "" }
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/framework-bundle": "~2.3",
        "symfony/console": "~2.3",
        "symfony/finder": "~2.3",
        "knplabs/knp-snappy-bundle": "v1.1"
        
    },
    "target-dir": "Mhor/CvToPdfBundle",
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    }
}
